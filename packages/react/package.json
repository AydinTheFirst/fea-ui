{
  "name": "@fea-ui/react",
  "version": "0.1.0-alpha.3",
  "description": "FeaUI React - A lightweight, accessible React component library built on top of Base UI",
  "keywords": [
    "fea-ui",
    "react",
    "ui",
    "design-system",
    "components",
    "tailwind",
    "library"
  ],
  "homepage": "https://feaui.aydinthefirst.com",
  "bugs": {
    "url": "https://github.com/aydinthefirst/fea-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aydinthefirst/fea-ui.git"
  },
  "license": "MIT",
  "author": "Halil AydÄ±n <aydinhalil980@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "add:component": "bun scripts/add-component.mjs",
    "postadd:component": "bun run gen:exports",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "gen:exports": "bun scripts/generate-exports.mjs",
    "postgen:exports": "bun lint",
    "lint": "biome check --write",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "lucide-react": "^0.562.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/bun": "^1.3.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsdown": "^0.18.2",
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "react": ">19.0.0",
    "react-dom": ">19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
